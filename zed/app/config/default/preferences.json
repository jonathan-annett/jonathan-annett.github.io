{
    "preferences": {
        "animatedScroll": true,
        "autoIndentOnPaste": true,
        "behaviorsEnabled": true,
        "continuousCompletion": false,
        "continuousCompletionDelay": 250,
        "autoTriggerCompletion": true,
        "defaultMode": "ace/mode/text",
        "detectIndentation": true,
        "dimInactiveEditors": true,
        "displayIndentGuides": true,
        "editorTheme": "zed_dark",
        "fontFamily": "'Monaco', 'Menlo', 'Ubuntu Mono', 'Consolas', 'source-code-pro', monospace",
        "fontSize": 12,
        "globalAutoRevert": true,
        "gotoExclude": ["*/.git/*", "*/.zedstate", "*/.DS_Store"],
        "highlightActiveLine": true,
        "highlightGutterLine": true,
        "highlightSelectedWord": true,
        "hygienicMode": false,
        "hygienicModeRemote": true,
        "keybinding": "zed",
        "nativeScrollBars": false,
        "persistentTree": false,
        "previewDelay": 500,
        "printMarginColumn": 80,
        "recentFolderHistory": 10,
        "saveTimeout": 2000,
        "scrollPastEnd": false,
        "scrollSpeed": 2,
        "showFoldWidgets": true,
        "showGutter": true,
        "showInvisibles": false,
        "showPrintMargin": true,
        "tabSize": 4,
        "trimEmptyLines": true,
        "trimWhitespaceOnSave": false,
        "useSoftTabs": true,
        "windowTheme": "zed_dark",
        "wordWrap": false,
        "wordWrapColumn": null,
        "wrapBehaviorsEnabled": false,
        "zedremServer": "wss://remote.zedapp.org:443"
    }
}
