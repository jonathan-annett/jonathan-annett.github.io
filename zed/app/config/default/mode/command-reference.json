{
    "modes": {
        "command-reference": {
            "name": "Zed Command Reference",
            "highlighter": "ace/mode/markdown",
            "filenames": ["zed::commands"],

            "keys": {
                "Zed:Command Reference:Invoke Command": "Enter"
            },

            "commands": {
                "Zed:Command Reference:Invoke Command": {
                    "scriptUrl": "/default/mode/command-reference/invoke.js",
                    "inputs": {
                        "lines": true,
                        "cursor": true
                    },
                    "internal": true
                }
            },

            "preferences": {
                "showGutter": false,
                "showInvisibles": false,
                "showPrintMargin": false,
                "wordWrap": true,
                "wordWrapColumn": 75
            },

            "handlers": {
                "click": ["Zed:Command Reference:Invoke Command"]
            }
        }
    }
}
