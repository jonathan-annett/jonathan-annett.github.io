{
    modes: {
        java: {
            name: "Java",
            highlighter: "ace/mode/java",
            extensions: [
                "java"
            ],
            completionTriggers: ["."],
            commands: {
                "Tools:Index": {
                    scriptUrl: "/default/command/regex_indexer.js",
                    inputs: {
                        text: true
                    },
                    regexes: [{
                        regex: "[a-zA-Z0-9_\\$]+\\s+([a-zA-Z0-9_\\$]+)\\s*\\([^\\)]*\\)\\s*\\{",
                        symbolIndex: 1,
                        type: "function"
                    }, {
                        regex: "class\\s+([a-zA-Z0-9_\\$]+)\\s*(extends|implements|\\{)",
                        symbolIndex: 1,
                        type: "type"
                    }],
                    sandbox: "index"
                }
            },

            handlers: {
                index: ["Tools:Index"]
            }
        }
    }
}
